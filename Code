predictors_test_nonwhite <- test_nonwhite %>% select(AdmQtr, OperYR, Age, SEX, BMI, RACE_NEW, ETHNICITY_HISPANIC, ORIGIN, HYPERMED, DIABETES, SMOKE_CURRENT, HXCHF, HXCOPD, DIALYSIS, FNSTATUS2, PHYSHIGHRISK, ASPIRIN, STATIN, BETAB, PRSODM_mmolL, PRBUN_mmolL, PRCREAT_umolL, PRALBUM_gL, PRWBC_Kmm3, PRHCT_LL, PRPLATE_Kcumm, PRINR, PRPTT_s, LEO_HEMO, ANATHIGHRISK, CONCUR_MIN_AMP, CONCUR_ENDO_ILIAC, CONCUR_ENDO_INFRA, LEO_SYMPT, LEO_PROC, URGENCY, ASACLAS)

