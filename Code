plot(perf_asympt_claud, add = TRUE, col = 'blue')
abline(a=0,b=1)

#Subgroup analysis based on procedure type
test_fempop <- test[test$LEO_PROC<=2, ]
test_femtib <- test[test$LEO_PROC==3 & LEO_PROC==4, ]
test_poptib <- test[test$LEO_PROC==5 & LEO_PROC==6, ]
test_endart <- test[test$LEO_PROC==7 & LEO_PROC==8, ]
predictors_test_fempop <- test_fempop %>% select(AdmQtr, OperYR, Age, SEX, BMI, RACE_NEW, ETHNICITY_HISPANIC, ORIGIN, HYPERMED, DIABETES, SMOKE_CURRENT, HXCHF, HXCOPD, DIALYSIS, FNSTATUS2, PHYSHIGHRISK, ASPIRIN, STATIN, BETAB, PRSODM_mmolL, PRBUN_mmolL, PRCREAT_umolL, PRALBUM_gL, PRWBC_Kmm3, PRHCT_LL, PRPLATE_Kcumm, PRINR, PRPTT_s, LEO_HEMO, ANATHIGHRISK, CONCUR_MIN_AMP, CONCUR_ENDO_ILIAC, CONCUR_ENDO_INFRA, LEO_SYMPT, LEO_PROC, URGENCY, ASACLAS)
predictors_test_femtib <- test_femtib %>% select(AdmQtr, OperYR, Age, SEX, BMI, RACE_NEW, ETHNICITY_HISPANIC, ORIGIN, HYPERMED, DIABETES, SMOKE_CURRENT, HXCHF, HXCOPD, DIALYSIS, FNSTATUS2, PHYSHIGHRISK, ASPIRIN, STATIN, BETAB, PRSODM_mmolL, PRBUN_mmolL, PRCREAT_umolL, PRALBUM_gL, PRWBC_Kmm3, PRHCT_LL, PRPLATE_Kcumm, PRINR, PRPTT_s, LEO_HEMO, ANATHIGHRISK, CONCUR_MIN_AMP, CONCUR_ENDO_ILIAC, CONCUR_ENDO_INFRA, LEO_SYMPT, LEO_PROC, URGENCY, ASACLAS)
predictors_test_poptib <- test_poptib %>% select(AdmQtr, OperYR, Age, SEX, BMI, RACE_NEW, ETHNICITY_HISPANIC, ORIGIN, HYPERMED, DIABETES, SMOKE_CURRENT, HXCHF, HXCOPD, DIALYSIS, FNSTATUS2, PHYSHIGHRISK, ASPIRIN, STATIN, BETAB, PRSODM_mmolL, PRBUN_mmolL, PRCREAT_umolL, PRALBUM_gL, PRWBC_Kmm3, PRHCT_LL, PRPLATE_Kcumm, PRINR, PRPTT_s, LEO_HEMO, ANATHIGHRISK, CONCUR_MIN_AMP, CONCUR_ENDO_ILIAC, CONCUR_ENDO_INFRA, LEO_SYMPT, LEO_PROC, URGENCY, ASACLAS)
predictors_test_endart <- test_endart %>% select(AdmQtr, OperYR, Age, SEX, BMI, RACE_NEW, ETHNICITY_HISPANIC, ORIGIN, HYPERMED, DIABETES, SMOKE_CURRENT, HXCHF, HXCOPD, DIALYSIS, FNSTATUS2, PHYSHIGHRISK, ASPIRIN, STATIN, BETAB, PRSODM_mmolL, PRBUN_mmolL, PRCREAT_umolL, PRALBUM_gL, PRWBC_Kmm3, PRHCT_LL, PRPLATE_Kcumm, PRINR, PRPTT_s, LEO_HEMO, ANATHIGHRISK, CONCUR_MIN_AMP, CONCUR_ENDO_ILIAC, CONCUR_ENDO_INFRA, LEO_SYMPT, LEO_PROC, URGENCY, ASACLAS)

pred_fempop <- predict(XGB, as.matrix(predictors_test_fempop))
auc(test_fempop$MALE, pred_fempop)
ci.auc(test_fempop$MALE, pred_fempop)

pred_femtib <- predict(XGB, as.matrix(predictors_test_femtib))
auc(test_femtib$MALE, pred_femtib)
ci.auc(test_femtib$MALE, pred_femtib)

pred_poptib <- predict(XGB, as.matrix(predictors_test_poptib))
auc(test_poptib$MALE, pred_poptib)
ci.auc(test_poptib$MALE, pred_poptib)

pred_endart <- predict(XGB, as.matrix(predictors_test_endart))
auc(test_endart$MALE, pred_endart)
ci.auc(test_endart$MALE, pred_endart)

perf_fempop <- prediction(pred_fempop, test_fempop$MALE)
perf_fempop <- performance(perf_fempop, "tpr", "fpr")

perf_femtib <- prediction(pred_femtib, test_femtib$MALE)
perf_femtib <- performance(perf_femtib, "tpr", "fpr")

perf_poptib <- prediction(pred_poptib, test_poptib$MALE)
perf_poptib <- performance(perf_poptib, "tpr", "fpr")

perf_endart <- prediction(pred_endart, test_endart$MALE)
perf_endart <- performance(perf_endart, "tpr", "fpr")

plot(perf_femopop)
plot(perf_femtib, add = TRUE, col = 'blue')
plot(perf_poptib, add = TRUE, col = 'orange')
plot(perf_endart, add = TRUE, col = 'green')
abline(a=0,b=1)

#Subgroup analysis based on urgency of surgery
test_urgent <- test[test$URGENCY!=1, ]
test_elective <- test[test$URGENCY==1, ]
predictors_test_urgent <- test_urgent %>% select(AdmQtr, OperYR, Age, SEX, BMI, RACE_NEW, ETHNICITY_HISPANIC, ORIGIN, HYPERMED, DIABETES, SMOKE_CURRENT, HXCHF, HXCOPD, DIALYSIS, FNSTATUS2, PHYSHIGHRISK, ASPIRIN, STATIN, BETAB, PRSODM_mmolL, PRBUN_mmolL, PRCREAT_umolL, PRALBUM_gL, PRWBC_Kmm3, PRHCT_LL, PRPLATE_Kcumm, PRINR, PRPTT_s, LEO_HEMO, ANATHIGHRISK, CONCUR_MIN_AMP, CONCUR_ENDO_ILIAC, CONCUR_ENDO_INFRA, LEO_SYMPT, LEO_PROC, URGENCY, ASACLAS)
predictors_test_elective <- test_elective %>% select(AdmQtr, OperYR, Age, SEX, BMI, RACE_NEW, ETHNICITY_HISPANIC, ORIGIN, HYPERMED, DIABETES, SMOKE_CURRENT, HXCHF, HXCOPD, DIALYSIS, FNSTATUS2, PHYSHIGHRISK, ASPIRIN, STATIN, BETAB, PRSODM_mmolL, PRBUN_mmolL, PRCREAT_umolL, PRALBUM_gL, PRWBC_Kmm3, PRHCT_LL, PRPLATE_Kcumm, PRINR, PRPTT_s, LEO_HEMO, ANATHIGHRISK, CONCUR_MIN_AMP, CONCUR_ENDO_ILIAC, CONCUR_ENDO_INFRA, LEO_SYMPT, LEO_PROC, URGENCY, ASACLAS)

pred_urgent <- predict(XGB, as.matrix(predictors_test_urgent))
auc(test_urgent$MALE, pred_urgent)
ci.auc(test_urgent$MALE, pred_urgent)

pred_elective <- predict(XGB, as.matrix(predictors_test_elective))
auc(test_elective$MALE, pred_elective)
ci.auc(test_elective$MALE, pred_elective)

perf_urgent <- prediction(pred_urgent, test_urgent$MALE)
perf_urgent <- performance(perf_urgent, "tpr", "fpr")

perf_elective <- prediction(pred_elective, test_elective$MALE)
perf_elective <- performance(perf_elective, "tpr", "fpr")

plot(perf_urgent)
plot(perf_elective, add = TRUE, col = 'blue')
abline(a=0,b=1)

#Train and evaluate XGBoost for predicting secondary outcomes

XGB_loss_pat <- xgboost(data = as.matrix(predictors_train), label = as.numeric(train$loss_pat), nrounds = 100, verbose = TRUE, objective = 'binary:logistic', eval_metric = 'auc', max_depth = 6, eta = 0.3, gamma = 0, colsample_bytree = 1, min_child_weight = 1, subsample = 1) #hyperparameters chosen based on above grid search and cross validation
XGB_loss_pat_pred <- predict(XGB_loss_pat, as.matrix(predictors_test))
auc(test$loss_pat, XGB_loss_pat_pred)
ci.auc(test$loss_pat, XGB_loss_pat_pred)
XGB_loss_pat_pred_0.5 <- ifelse(XGB_loss_pat_pred > 0.5,1,0)
confusionMatrix(as.factor(test$loss_pat), as.factor(XGB_loss_pat_pred_0.5))

XGB_reint <- xgboost(data = as.matrix(predictors_train), label = as.numeric(train$reint), nrounds = 100, verbose = TRUE, objective = 'binary:logistic', eval_metric = 'auc', max_depth = 6, eta = 0.3, gamma = 0, colsample_bytree = 1, min_child_weight = 1, subsample = 1)
