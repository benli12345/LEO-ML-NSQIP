#R version 4.2.1
#Load NSQIP data (available with approval through ACS https://www.facs.org/quality-programs/data-and-registries/acs-nsqip/participant-use-data-file/)
library(readr)
data <- read_csv("LEO_NSQIP_data.csv")

#Split data into train (70%) and test (30%) sets
library(caTools)
set.seed(123)
sample <- sample.split(data$MALE, SplitRatio = 0.7)
train <- subset(data, sample == TRUE)
test <- subset(data, sample == FALSE)

#Apply Random Over-Sample Examples (ROSE) for class balance on training set
library(ROSE)
train <- ROSE(MALE ~ ., data = train, N = 10000, seed = 123)$data

#Define predictors
library(dplyr)
predictors_train <- train %>% select(AdmQtr, OperYR, Age, SEX, BMI, RACE_NEW, ETHNICITY_HISPANIC, ORIGIN, HYPERMED, DIABETES, SMOKE_CURRENT, HXCHF, HXCOPD, DIALYSIS, FNSTATUS2, PHYSHIGHRISK, ASPIRIN, STATIN, BETAB, PRSODM_mmolL, PRBUN_mmolL, PRCREAT_umolL, PRALBUM_gL, PRWBC_Kmm3, PRHCT_LL, PRPLATE_Kcumm, PRINR, PRPTT_s, LEO_HEMO, ANATHIGHRISK, CONCUR_MIN_AMP, CONCUR_ENDO_ILIAC, CONCUR_ENDO_INFRA, LEO_SYMPT, LEO_PROC, URGENCY, ASACLAS)
predictors_test <- test %>% select(AdmQtr, OperYR, Age, SEX, BMI, RACE_NEW, ETHNICITY_HISPANIC, ORIGIN, HYPERMED, DIABETES, SMOKE_CURRENT, HXCHF, HXCOPD, DIALYSIS, FNSTATUS2, PHYSHIGHRISK, ASPIRIN, STATIN, BETAB, PRSODM_mmolL, PRBUN_mmolL, PRCREAT_umolL, PRALBUM_gL, PRWBC_Kmm3, PRHCT_LL, PRPLATE_Kcumm, PRINR, PRPTT_s, LEO_HEMO, ANATHIGHRISK, CONCUR_MIN_AMP, CONCUR_ENDO_ILIAC, CONCUR_ENDO_INFRA, LEO_SYMPT, LEO_PROC, URGENCY, ASACLAS)

#Train pre-op models
library(caret)
library(xgboost)
library(ranger)
library(naivebayes)
library(e1071)
library(nnet)
train_control <- trainControl(method = "cv", number = 10) #10-fold cross-validation

#Train XGBoost
XGBgrid <- expand.grid(max_depth = c(2,3,4,5,6,7,8,9), nrounds = (1:10)*50, eta = c(0.4,0.3,0.2,0.1,0.05,0.01,0.001), gamma = c(0,0.1,1,1.5,2), subsample = c(0.5,0.6,0.7,0.8,0.9,1), min_child_weight = c(1,3,5,7,10), colsample_bytree = c(0.5,0.6,0.7,0.8,0.9,1) #XGBoost grid search for hyperparameters
XGB <- train(x = as.matrix(predictors_train), y = train$MALE, data = train, method = "xgbTree", metric = "ROC", trControl = train_control, tuneGrid = XGBgrid)

#Train random forest
RFgrid <- expand.grid(mtry = 2:4, splitrule = "gini", min.node.size = c(10,20))
RF <- train(x = predictors_train, y = train$MALE, data = train, method = "ranger", metric = "ROC", trControl = train_control, tuneGrid = RFgrid)

#Train naive bayes
NBgrid <- expand.grid(usekernel = c(TRUE, FALSE), fL = 0:5, adjust = seq(0,5,by=1))
NB <- train(x = predictors_train, y = as.factor(train$MALE), data = train, method = "naive_bayes", metric = "ROC", trControl = train_control, tuneGrid = NBgrid)

#Train support vector machine
SVMgrid <- expand.grid(C = c(0.001, 0.01, 0.1, 1, 10, 100, 10000))
SVM <- train(x = predictors_train, y = train$MALE, data = train, method = "svmRadial", metric = "ROC", trControl = train_control, tuneGrid = SVMgrid)

#Train artificial neural network
NNETgrid <- expand.grid(size = seq(1,10,by=1), decay = seq(0.1,0.5,by=0.1))
NNET <- train(x = predictors_train, y = train$MALE, data = train, method = "nnet", metric = "ROC", trControl = train_control, tuneGrid = NNETgrid)

#Train logistic regression
LRgrid <- expand.grid(C = c(0.001, 0.01, 0.1, 1, 10, 100, 10000))
LR <- train(x = predictors_train, y = train$MALE, data = train, method = "glm", metric = "ROC", trControl = train_control, tuneGrid = LRgrid, family = "binomial")

#Evaluate models on test set
library(pROC)

XGB_pred <- predict(XGB, as.matrix(predictors_test))
auc(test$MALE, XGB_pred)
ci.auc(test$MALE, XGB_pred)
XGB_pred_0.5 <- ifelse(XGB_pred > 0.5,1,0)
confusionMatrix(as.factor(test$MALE), as.factor(XGB_pred_0.5))

RF_pred <- predict(RF, predictors_test)
auc(test$MALE, RF_pred$predictions)
ci.auc(test$MALE, RF_pred$predictions)
RF_pred_0.5 <- ifelse(RF_pred$predictions > 0.5,1,0)
confusionMatrix(as.factor(test$MALE), as.factor(RF_pred_0.5))

NB_pred <- predict(NB, predictors_test)
auc(test$MALE, as.numeric(NB_pred))
ci.auc(test$MALE, as.numeric(NB_pred))
NB_pred_0.5 <- ifelse(as.numeric(NB_pred) > 0.5,1,0)
confusionMatrix(as.factor(test$MALE), as.factor(NB_pred_0.5))

SVM_pred <- predict(SVM, predictors_test)
auc(test$MALE, SVM_pred)
ci.auc(test$MALE, SVM_pred)
SVM_pred_0.5 <- ifelse(SVM_pred > 0.5,1,0)
confusionMatrix(as.factor(test$MALE), as.factor(SVM_pred_0.5))

NNET_pred <- predict(NNET, predictors_test)
auc(test$MALE, NNET_pred)
ci.auc(test$MALE, NNET_pred)
NNET_pred_0.5 <- ifelse(NNET_pred > 0.5,1,0)
confusionMatrix(as.factor(test$MALE), as.factor(NNET_pred_0.5))

LR_pred <- predict(LR, predictors_test)
auc(test$MALE, LR_pred)
ci.auc(test$MALE, LR_pred)
LR_pred_0.5 <- ifelse(LR_pred > 0.5,1,0)
confusionMatrix(as.factor(test$MALE), as.factor(LR_pred_0.5))

#XGBoost selected as best performing model

#Generate ROC curves
library(ROCR)
XGB_perf <- prediction(XGB_pred, test$MALE)
XGB_perf <- performance(XGB_perf, "tpr", "fpr")
plot(XGB_perf)
abline(a=0,b=1)

#Generate calibration plot
library(gbm)
calibrate.plot(test$MALE, XGB_pred)

#Calculate Brier Score
library(DescTools)
BrierScore(XGB_pred, test$MALE)

#Calculate variable importance scores for top 10 predictors
importance_matrix <- xgb.importance(names(predictors_test), model = XGB)
xgb.plot.importance(importance_matrix, top_n = 10)

#Subgroup analysis of variable importance scores for top 10 predictors based on symptom status

test_CLTI <- test[test$LEO_SYMPT>=3, ]
predictors_test_CLTI <- test_CLTI %>% select(AdmQtr, OperYR, Age, SEX, BMI, RACE_NEW, ETHNICITY_HISPANIC, ORIGIN, HYPERMED, DIABETES, SMOKE_CURRENT, HXCHF, HXCOPD, DIALYSIS, FNSTATUS2, PHYSHIGHRISK, ASPIRIN, STATIN, BETAB, PRSODM_mmolL, PRBUN_mmolL, PRCREAT_umolL, PRALBUM_gL, PRWBC_Kmm3, PRHCT_LL, PRPLATE_Kcumm, PRINR, PRPTT_s, LEO_HEMO, ANATHIGHRISK, CONCUR_MIN_AMP, CONCUR_ENDO_ILIAC, CONCUR_ENDO_INFRA, LEO_SYMPT, LEO_PROC, URGENCY, ASACLAS)
importance_matrix <- xgb.importance(names(predictors_test_CLTI), model = XGB)
xgb.plot.importance(importance_matrix, top_n = 10)

test_asympt_claud <- test[test$LEO_SYMPT<=2, ]
predictors_test_asympt_claud <- test_asympt_claud %>% select(AdmQtr, OperYR, Age, SEX, BMI, RACE_NEW, ETHNICITY_HISPANIC, ORIGIN, HYPERMED, DIABETES, SMOKE_CURRENT, HXCHF, HXCOPD, DIALYSIS, FNSTATUS2, PHYSHIGHRISK, ASPIRIN, STATIN, BETAB, PRSODM_mmolL, PRBUN_mmolL, PRCREAT_umolL, PRALBUM_gL, PRWBC_Kmm3, PRHCT_LL, PRPLATE_Kcumm, PRINR, PRPTT_s, LEO_HEMO, ANATHIGHRISK, CONCUR_MIN_AMP, CONCUR_ENDO_ILIAC, CONCUR_ENDO_INFRA, LEO_SYMPT, LEO_PROC, URGENCY, ASACLAS)
importance_matrix <- xgb.importance(names(predictors_test_asympt_claud), model = XGB)
xgb.plot.importance(importance_matrix, top_n = 10)

#Subgroup analysis based on age
test_under70 <- test[test$Age<70, ]
test_over70 <- test[test$Age>70, ]
predictors_test_under70 <- test_under70 %>% select(AdmQtr, OperYR, Age, SEX, BMI, RACE_NEW, ETHNICITY_HISPANIC, ORIGIN, HYPERMED, DIABETES, SMOKE_CURRENT, HXCHF, HXCOPD, DIALYSIS, FNSTATUS2, PHYSHIGHRISK, ASPIRIN, STATIN, BETAB, PRSODM_mmolL, PRBUN_mmolL, PRCREAT_umolL, PRALBUM_gL, PRWBC_Kmm3, PRHCT_LL, PRPLATE_Kcumm, PRINR, PRPTT_s, LEO_HEMO, ANATHIGHRISK, CONCUR_MIN_AMP, CONCUR_ENDO_ILIAC, CONCUR_ENDO_INFRA, LEO_SYMPT, LEO_PROC, URGENCY, ASACLAS)
predictors_test_over70 <- test_over70 %>% select(AdmQtr, OperYR, Age, SEX, BMI, RACE_NEW, ETHNICITY_HISPANIC, ORIGIN, HYPERMED, DIABETES, SMOKE_CURRENT, HXCHF, HXCOPD, DIALYSIS, FNSTATUS2, PHYSHIGHRISK, ASPIRIN, STATIN, BETAB, PRSODM_mmolL, PRBUN_mmolL, PRCREAT_umolL, PRALBUM_gL, PRWBC_Kmm3, PRHCT_LL, PRPLATE_Kcumm, PRINR, PRPTT_s, LEO_HEMO, ANATHIGHRISK, CONCUR_MIN_AMP, CONCUR_ENDO_ILIAC, CONCUR_ENDO_INFRA, LEO_SYMPT, LEO_PROC, URGENCY, ASACLAS)

pred_under70 <- predict(XGB, as.matrix(predictors_test_under70))
auc(test_under70$MALE, pred_under70)
ci.auc(test_under70$MALE, pred_under70)

pred_over70 <- predict(XGB, as.matrix(predictors_test_over70))
auc(test_over70$MALE, pred_over70)
ci.auc(test_over70$MALE, pred_over70)

perf_under70 <- prediction(pred_under70, test_under70$MALE)
perf_under70 <- performance(perf_under70, "tpr", "fpr")

perf_over70 <- prediction(pred_over70, test_over70$MALE)
perf_over70 <- performance(perf_over70, "tpr", "fpr")

plot(perf_under70)
plot(perf_over70, add = TRUE, col = 'blue')
abline(a=0,b=1)

#Subgroup analysis based on sex
test_male <- test[test$SEX==1, ]
test_female <- test[test$SEX==2, ]
predictors_test_male <- test_male %>% select(AdmQtr, OperYR, Age, SEX, BMI, RACE_NEW, ETHNICITY_HISPANIC, ORIGIN, HYPERMED, DIABETES, SMOKE_CURRENT, HXCHF, HXCOPD, DIALYSIS, FNSTATUS2, PHYSHIGHRISK, ASPIRIN, STATIN, BETAB, PRSODM_mmolL, PRBUN_mmolL, PRCREAT_umolL, PRALBUM_gL, PRWBC_Kmm3, PRHCT_LL, PRPLATE_Kcumm, PRINR, PRPTT_s, LEO_HEMO, ANATHIGHRISK, CONCUR_MIN_AMP, CONCUR_ENDO_ILIAC, CONCUR_ENDO_INFRA, LEO_SYMPT, LEO_PROC, URGENCY, ASACLAS)
predictors_test_female <- test_female %>% select(AdmQtr, OperYR, Age, SEX, BMI, RACE_NEW, ETHNICITY_HISPANIC, ORIGIN, HYPERMED, DIABETES, SMOKE_CURRENT, HXCHF, HXCOPD, DIALYSIS, FNSTATUS2, PHYSHIGHRISK, ASPIRIN, STATIN, BETAB, PRSODM_mmolL, PRBUN_mmolL, PRCREAT_umolL, PRALBUM_gL, PRWBC_Kmm3, PRHCT_LL, PRPLATE_Kcumm, PRINR, PRPTT_s, LEO_HEMO, ANATHIGHRISK, CONCUR_MIN_AMP, CONCUR_ENDO_ILIAC, CONCUR_ENDO_INFRA, LEO_SYMPT, LEO_PROC, URGENCY, ASACLAS)

pred_male <- predict(XGB, as.matrix(predictors_test_male))
